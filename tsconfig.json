{
  "compilerOptions": {
    "noImplicitReturns": true,
    "noUnusedLocals": false,
    "outDir": "dist",
    "sourceMap": true,
    "removeComments": false,
    "strict": true,
    "target": "ESNext",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "allowJs": true
  },
  "compileOnSave": true,
  "include": ["examples", "tsconfig.json"]
}
